    const DeanNavHandler = (e) => {
        const allRecords = document.getElementById('UserWithRecords');
        const evaluatorRecords = document.getElementById('EvaluatorRecords');
        const searchRecords = document.getElementById('SearchRecords');
        const filterRecords = document.getElementById('FilterRecords');

        allRecords.style.display = "none";
        evaluatorRecords.style.display = "none";
        searchRecords.style.display = "none";
        filterRecords.style.display = "none";

        switch (e.target.id) {
            case "AllRecordsIcon":
                allRecords.style.display = "grid";
                getRecords();
                break;
            case "EvaluatorIcon":
                evaluatorRecords.style.display = "grid";
                break;
            case "SearchIcon":
                searchRecords.style.display = "grid";
                break;
            case "FilterIcon":
                filterRecords.style.display = "grid";
                break;
            default:
                break;
        }
    }
